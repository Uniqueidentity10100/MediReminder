{
  "name": "medireminder",
  "version": "1.0.0",
  "description": "Healthcare medication reminder and adherence tracking application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": ["healthcare", "medication", "reminder", "adherence"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "apollo-server-express": "^3.12.0",
    "graphql": "^16.8.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "validator": "^13.11.0",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
